<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:animateLayoutChanges="true"
    android:background="#FFFFFF"
    tools:context=".StartActivity">

    <VideoView
        android:id="@+id/videoView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/image_textlogo"
        android:layout_width="235dp"
        android:layout_height="55dp"
        android:layout_marginTop="200dp"
        android:src="@drawable/logo_text"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/image_garagelogo"
        android:layout_width="180dp"
        android:layout_height="140dp"
        android:layout_marginTop="50dp"
        android:rotation="5"
        android:rotationX="0"
        android:rotationY="0"
        android:src="@drawable/logo_image"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image_textlogo" />

    <Button
        android:id="@+id/button_start"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginBottom="130dp"
        android:background="@drawable/buttoncolor2"
        android:elevation="2dp"
        android:text="@string/start"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/White"
        android:textSize="20sp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button_demo"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/buttonwhite"
        android:elevation="2dp"
        android:stateListAnimator="@android:anim/fade_in"
        android:text="@string/startDemo"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/buttonwhitetextchange"
        android:textSize="20sp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>